<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.csse_mobileapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewRequisition"></activity>
        <activity android:name=".AddOrder" />
        <activity android:name=".AddItems" />
        <activity android:name=".OrderSummary" />
        <activity android:name=".VendorDetails" />
        <activity android:name=".SiteDetails" />
        <activity android:name=".ViewStock" />
        <activity android:name=".SiteManagerLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>